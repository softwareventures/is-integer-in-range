{
  "name": "is-integer-in-range",
  "version": "0.0.0-development",
  "description": "Tests if a value is an integer in a specified range",
  "keywords": [
    "TypeScript",
    "integer",
    "integer-range",
    "is-integer-in-range",
    "number",
    "range"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/is-integer-in-range",
  "bugs": "https://github.com/softwareventures/is-integer-in-range",
  "repository": "github:softwareventures/is-integer-in-range",
  "license": "ISC",
  "scripts": {
    "pre-commit": "precise-commits",
    "fix": "tsc --noEmit && eslint --fix . && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check .",
    "prepare": "husky install && tsc",
    "semantic-release": "semantic-release",
    "test": "ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@types/is-integer": "^1.0.0",
    "is-integer": "^1.0.7",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@softwareventures/eslint-config": "5.1.0",
    "@softwareventures/precise-commits": "2.0.18",
    "@softwareventures/prettier-config": "3.0.0",
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "6.0.0",
    "ava": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "semantic-release": "17.4.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@softwareventures"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
