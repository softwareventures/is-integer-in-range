{
  "name": "is-integer-in-range",
  "version": "0.0.0-development",
  "description": "Tests if a value is an integer in a specified range",
  "keywords": [
    "TypeScript",
    "integer",
    "integer-range",
    "is-integer-in-range",
    "number",
    "range"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/is-integer-in-range",
  "bugs": "https://github.com/softwareventures/is-integer-in-range",
  "repository": "github:softwareventures/is-integer-in-range",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@types/is-integer": "^1.0.0",
    "is-integer": "^1.0.7",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "5.1.0",
    "@softwareventures/tslint-rules": "1.0.0",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.5",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
